@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include(FindPkgConfig)
find_dependency(Qt5 COMPONENTS Core)

if (APPLE)
   set(PKG_CONFIG_EXECUTABLE pkg-config --define-prefix @LIBARCHIVE_PKG_CONFIG@)
   find_package(PkgConfig REQUIRED)
   pkg_check_modules(LIBARCHIVE REQUIRED libarchive IMPORTED_TARGET)
else()
   find_dependency(LibArchive)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/QArchiveTargets.cmake")
